<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="887dp"
        android:layout_height="1286dp"
        android:tint="?android:attr/textColorTertiaryInverse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bgi" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="541dp"
        android:layout_height="384dp"
        app:srcCompat="@drawable/prm"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="641dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="541dp"
        android:layout_height="298dp"
        app:srcCompat="@drawable/prm"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="565dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="42dp"
        android:layout_height="32dp"
        app:srcCompat="@drawable/ico"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="701dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="45dp"
        android:layout_height="37dp"
        app:srcCompat="@drawable/lock"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="816dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="1dp"
        android:layout_height="86dp"
        app:srcCompat="@drawable/line"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="669dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="1dp"
        android:layout_height="87dp"
        app:srcCompat="@drawable/line"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="788dp" />

    <EditText
        android:id="@+id/editTextNumberSigned"
        android:layout_width="446dp"
        android:layout_height="84dp"
        android:ems="10"
        android:inputType="numberSigned"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="671dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Forgot Password?"
        tools:layout_editor_absoluteX="344dp"
        tools:layout_editor_absoluteY="932dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="520dp"
        android:layout_height="108dp"
        android:text="SIGN IN"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="984dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create A New Account?"
        tools:layout_editor_absoluteX="327dp"
        tools:layout_editor_absoluteY="1168dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="442dp"
        android:layout_height="85dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="216dp"
        tools:layout_editor_absoluteY="788dp" />

</androidx.constraintlayout.widget.ConstraintLayout>